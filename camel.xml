<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:c="http://camel.apache.org/schema/spring"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

<c:camelContext>

	<c:route>
		<c:from uri="rabbitmq://localhost/rabbitOut?queue=rabbitQueueOut&amp;autoDelete=false"/>
		<c:bean ref="MyProcessor"/>
		<c:to uri="log:RABBIT-LOGGER?level=WARN"/>
		<c:to uri="solace-jcsmp:topic:sol/in"/>
	</c:route>

	<c:route>
		<c:from uri="solace-jcsmp:queue:solaceOut"/>
		<c:bean ref="MyProcessor"/>
		<c:to uri="log:RABBIT-LOGGER?level=WARN"/>
		<c:to uri="rabbitmq://localhost/rabbitIn?queue=rabbitQueueIn&amp;autoDelete=false"/>
	</c:route>

</c:camelContext>

</beans>
